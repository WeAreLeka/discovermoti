
{{> site/head }}

	<body>

		<!-- <div id="landing&#45;background"></div> -->

		<div class="site-wrapper">

			<div class="site-wrapper-inner">

				<div class="cover-container">

					{{> pages/navbar }}

					{{{ body }}}

					{{> site/footer }}

				</div>

			</div>

		</div>

		<div class="vid">

