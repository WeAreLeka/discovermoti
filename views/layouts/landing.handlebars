				</video> <!-- splash -->

			</div> <!-- video-container -->

		</div> <!-- vid -->

		<!-- jQuery -->
		{{> site/analytics }}
		{{> site/jquery-js }}
		{{> pages/vid-js }}

	</body>

</html>
